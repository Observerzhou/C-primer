#include <iostream>
#include <vector>

using namespace std;

int main()
{
	int word;
	vector<int> vec;

	while(cin >> word)
		vec.push_back(word);

	if(vec.size() == 0){
		cout << "input atleast one integer" << endl;
		return -1;
	}
	else if(vec.sizeof() == 1)
	{
		cout << vec[0] << endl;
	}
	else
	{
		for(decltype(vec.size()) i=0; i!=vec.size()-1; ++i)
		cout << vec[i]+vec[i+1] << endl;
	}


	decltype(vec.size()) size = vec.size()
		if(size%2 = 1)
			size = size/2+1;
		else
			size = size/2;

	for(decltype(vec.size()) i=0; i!=size; i++)
		cout << vec[i]+vec[vec.size()-i-1] << endl;

	return 0;
}
